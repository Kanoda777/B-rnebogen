{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/css/*",
      "/fonts/*",
      "/images/*",
      "/js/*",
      "/favicon.ico",
      "/manifest.webmanifest",
      "/manifest.json",
      "/robots.txt",
      "/service-worker.js",
      "/sw.js",
      "/.auth/*",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
